{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "SupTube",
    "version": "1.2.2"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": false,
        "print": false,
        "setFullscreen": true,
        "setResizable": true
      },
      "dialog": {
        "all": false,
        "ask": false,
        "confirm": false,
        "message": false
      }
    },
    "bundle": {
      "active": true,
      "publisher": "Sandun Wiratunga",
      "category": "Entertainment",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.suptube.app",
      "longDescription": "An opensource client for YouTube",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "An opensource client for YouTube",
      "targets": "all",
      "appimage": {
        "bundleMediaFramework": true
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "allowDowngrades": false,
        "wix": {
          "language": "en-US",
          "dialogImagePath": "../src/assets/dialogImage.bmp",
          "bannerPath": "../src/assets/banner.bmp"
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": ["https://github.com/sandunwira/SupTube/releases/download/v{{current_version}}/SupTube_{{current_version}}_{{arch}}_en-US.msi"],
      "windows": {
        "installMode": "basicUi"
      },
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDFDMzZFMzBFRTRBRUVFOEEKUldTSzdxN2tEdU0ySEpzUWY3Y1kwN2VaMnNpU1kyZkdnSmNxQng4QmM3MTJmU2ZINjY1a090S3UK"
    },
    "windows": [
      {
        "title": "SupTube",
        "width": 1500,
        "height": 750,
        "url": "https://www.youtube.com",
        "fullscreen": false,
        "focus": true,
        "resizable": true,
        "decorations": true,
        "center": true,
        "visible": false
      },
      
      {
        "title": "SupTube",
        "width": 450,
